{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "> 0.25%, not dead",
          "node": "current"
        },
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3.8",
          "proposals": true
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "react-loadable/babel",
    "react-hot-loader/babel"
  ]
}